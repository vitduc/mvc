<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="nsk_part_numbers_interchange" resource="default" engine="innodb"
           comment="Nsk Part Numbers Interchange">
        <column xsi:type="int" name="entity_id" padding="10" unsigned="false" nullable="false" identity="true"
                comment="ID"/>
        <column xsi:type="varchar" name="competitor" nullable="false" length="40"
                comment="Competitor"/>
        <column xsi:type="varchar" name="part_number" nullable="false" length="40"
                comment="Part Number"/>
        <column xsi:type="varchar" name="nsk_part_number" nullable="false" length="40"
                comment="Nsk Part Number"/>
        <column xsi:type="varchar" name="note" nullable="true" length="255" comment="Note"/>
        <column xsi:type="tinyint" name="jp_flag" nullable="false" padding="2" default="0"
                comment="JAPAN FLAG"/>
        <column name="created_at" xsi:type="timestamp" nullable="false" comment="Created at"
                default="CURRENT_TIMESTAMP"/>
        <column name="updated_at" xsi:type="timestamp" nullable="false" comment="Updated at" on_update="true"
                default="CURRENT_TIMESTAMP"/>
        <constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="entity_id"/>
        </constraint>
        <constraint xsi:type="unique" referenceId="NSK_PART_NUMBERS_INTERCHANGE_ENTITY_ID_COMPETITOR_PART_NUMBER_NSK_PART_NUMBER">
            <column name="competitor"/>
            <column name="part_number"/>
            <column name="nsk_part_number"/>
        </constraint>
        <index referenceId="NSK_PART_NUMBERS_INTERCHANGE_PART_NUMBER" indexType="fulltext">
            <column name="part_number"/>
            <column name="competitor"/>
            <column name="note"/>
            <column name="nsk_part_number"/>
        </index>
    </table>
</schema>
